#include <iostream>
using namespace std;

int main() {
    int T,n;
    cin>>T;
    while(T--){
        cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++){
	        cin>>a[i];
	    }
	    int count=0;int count13=0;
	    for(int i=0;i<n;i++){
	        if(a[i]>=1&&a[i]<=7)count++;
	        count13++;
	        if(count==7){cout<<count13<<endl;
	        break;}
	    }
	
    }

	return 0;
}
